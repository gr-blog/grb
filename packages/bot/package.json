{
  "name": "bot",
  "version": "0.1.0",
  "private": true,
  "description": "Discord bot for GRB",
  "keywords": [],
  "author": "GregRos",
  "type": "module",
  "scripts": {
    "build": "tsc -b .",
    "build:clean": "run-s clean build",
    "clean": "shx rm -rf dist .tsbuildinfo || true",
    "start": "node dist/main.js",
    "watch": "tsc -b . --watch"
  },
  "lint-staged": {
    "*.{ts,mts,cts,js,mjs,cjs}": "node_modules/.bin/eslint --cache --fix",
    "'src/**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,ctsx,mtsx,html,htm,yaml,yml,json,scss,css}'": "node_modules/.bin/prettier --write"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.2",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.2",
    "@nestjs/platform-express": "^11.1.2",
    "@nestjs/swagger": "^11.2.0",
    "chalk": "^5.4.1",
    "dayjs": "^1.11.13",
    "doddle": "^5.0.13",
    "got": "^14.4.7",
    "lodash-es": "^4.17.21",
    "oceanic.js": "^1.12.0",
    "parse-duration": "^2.1.4",
    "qs": "^6.14.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "slugify": "^1.6.6",
    "what-are-you": "^0.1.12",
    "winston": "^3.17.0",
    "yamprint": "^0.8.4",
    "zod": "^3.25.51"
  },
  "devDependencies": {
    "@gregros/eslint-config": "^0.7.1",
    "@types/js-yaml": "^4",
    "@types/lodash": "^4.17.17",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.15.29",
    "@types/qs": "^6.14.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.3"
  },
  "packageManager": "yarn@4.9.2",
  "sourcesRoot": "src"
}
